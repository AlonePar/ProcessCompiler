  <bpmn2:process processType="Private" name="SubprocessExample" id="Process_1" isExecutable="true">
    
    <!-- nodes -->
    <bpmn2:startEvent id="StartEvent_1" name="StartProcess" />    
    <bpmn2:subProcess id="SubProcess_1" name="Sub Process 1">
    
      <!-- subprocess nodes -->
      <bpmn2:startEvent id="StartEvent_2" name="StartSubprocess" />
      <bpmn2:scriptTask id="ScriptTask_1" name="Script Task 1">
        <bpmn2:script>System.out.println("Running script inside the subprocess");</bpmn2:script>
      </bpmn2:scriptTask>
      <bpmn2:endEvent id="EndEvent_1" name="EndSubprocess"/>
      
      <!-- subprocess connections -->
      <bpmn2:sequenceFlow id="SequenceFlow_2" sourceRef="StartEvent_2" targetRef="ScriptTask_1"/>
      <bpmn2:sequenceFlow id="SequenceFlow_3" sourceRef="ScriptTask_1" targetRef="EndEvent_1"/>
      
    </bpmn2:subProcess>
    <bpmn2:scriptTask id="ScriptTask_2" name="Script Task 2">
      <bpmn2:script>System.out.println("Running script outside the subprocess");</bpmn2:script>
    </bpmn2:scriptTask>
    <bpmn2:endEvent id="EndEvent_2" name="EndProcess" />
    
    <!-- connections -->
    <bpmn2:sequenceFlow id="SequenceFlow_1" sourceRef="StartEvent_1" targetRef="SubProcess_1"/>
    <bpmn2:sequenceFlow id="SequenceFlow_4" sourceRef="SubProcess_1" targetRef="ScriptTask_2"/>    
    <bpmn2:sequenceFlow id="SequenceFlow_5" sourceRef="ScriptTask_2" targetRef="EndEvent_2"/>

  </bpmn2:process>
  
  
